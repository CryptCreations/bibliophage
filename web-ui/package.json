    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/devtools": "^8.0.3",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "daisyui": "^5.5.5",
